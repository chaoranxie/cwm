{
  "rules": {
    "routes": {
      ".read": true,
      ".write": "auth !== null",
      ".indexOn": "grade"
    },
    "routeCompletions": {
      ".read": false,
      ".write": false,
      "$uid": {
        ".write": "$uid === auth.uid",
        ".read": "$uid === auth.uid"
      }
    }
  }
}
